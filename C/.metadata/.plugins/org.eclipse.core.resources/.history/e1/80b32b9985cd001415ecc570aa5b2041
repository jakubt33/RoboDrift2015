#include <avr/io.h>
#include <avr/interrupt.h>
#include <util/delay.h>
#include <stdio.h>

#include "io.h"
#include "init.h"
#include "usart.h"

int main() {

	init_LED();
	init_USART(MYUBRR);

	while(1){
	_delay_ms(200);
	USART_Transmit(1);
	_delay_ms(200);
	USART_Transmit('2');


	}


}

