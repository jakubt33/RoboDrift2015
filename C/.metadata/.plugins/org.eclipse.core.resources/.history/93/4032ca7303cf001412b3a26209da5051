#include <avr/io.h>
#include <avr/interrupt.h>
#include <util/delay.h>
#include <stdio.h>

#include "io.h"
#include "init.h"


int main() {

	init_IO();
	init_ADC();

	TSAL_ENABLE_ON;
	while(1) {
		_delay_ms(20);
		if(bit_is_set(PIN_TSOP, TSOP_IN)){
			LED1_ON;
			LED2_OFF;
		}
		else {
			LED1_OFF;
			LED2_ON;
		}
	}

}
