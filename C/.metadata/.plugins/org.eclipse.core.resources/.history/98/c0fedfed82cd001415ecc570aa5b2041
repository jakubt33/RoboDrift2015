#include "io.h"
#include <avr/io.h>

void init_LED() {
	DDR_LED_Y |= (1 << LED1Y);
	DDR_LED_Y |= (1 << LED2Y);
	DDR_LED_Y |= (1 << LED3Y);
	DDR_LED_Y |= (1 << LED4Y);
	DDR_LED_Y |= (1 << LED5Y);

	DDR_LED_S124 |= (1 << LED1S);
	DDR_LED_S124 |= (1 << LED2S);
	DDR_LED_S35 |= (1 << LED3S);
	DDR_LED_S124 |= (1 << LED4S);
	DDR_LED_S35 |= (1 << LED5S);

	DDR_LED_FALSTART |= (1 << LED_FALSTART);
	DDR_LED_GO |= (1 << LED_GO);

	DDR_LED_RED |= (1 << LED_RED);
}

void init_USART(){

}

