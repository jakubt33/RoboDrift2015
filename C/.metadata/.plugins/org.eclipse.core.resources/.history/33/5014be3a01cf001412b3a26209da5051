#include <avr/io.h>
#include <avr/interrupt.h>
#include <util/delay.h>
#include <stdio.h>

#include "io.h"
#include "init.h"


int main() {

	init_IO();
	init_ADC();

	while(1) {
		_delay_ms(2000);
		LED_STRIPE_ON;
		TSAL_ENABLE_ON;
		LED1_ON;

		_delay_ms(2000);
		LED_STRIPE_OFF;
		TSAL_ENABLE_OFF;
		LED1_OFF;
	}

}
