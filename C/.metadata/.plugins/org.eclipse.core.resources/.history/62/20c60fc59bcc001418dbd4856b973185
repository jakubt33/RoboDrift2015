#ifndef IO_H_
#define IO_H_

//------------------------------------
#define DDR_LED DDRC

#define LED1S PC1
#define LED2S PC2
#define LED3S PC3
#define LED4S PC4
#define LED5S PC5

#define LED1S_OFF PORTC &= ~_BV(LED1);
#define LED1S_ON PORTC |= _BV(LED1);
#define LED2S_OFF PORTC &= ~_BV(LED2);
#define LED2S_ON PORTC |= _BV(LED2);
#define LED3S_OFF PORTC &= ~_BV(LED3);
#define LED3S_ON PORTC |= _BV(LED3);
#define LED4S_OFF PORTC &= ~_BV(LED4);
#define LED4S_ON PORTC |= _BV(LED4);
#define LED5S_OFF PORTC &= ~_BV(LED5);
#define LED5S_ON PORTC |= _BV(LED5);
//------------------------------------


#endif /* IO_H_ */
