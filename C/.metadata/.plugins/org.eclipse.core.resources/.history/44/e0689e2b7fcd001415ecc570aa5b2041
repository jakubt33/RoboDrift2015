#include "io.h"
#include <avr/io.h>

void init_LED() {
	DDR_LED_Y |= (1 << LED1Y);
	DDR_LED_Y |= (1 << LED2Y);
	DDR_LED_Y |= (1 << LED3Y);
	DDR_LED_Y |= (1 << LED4Y);
	DDR_LED_Y |= (1 << LED5Y);

	DDR_LED_FALSTART |= (1 << LED_FALSTART);
	DDR_LED_GO |= (1 << LED_GO);
}

